import java.util.*;
class NumberCheck{
	public static void main(String args[]){
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a number: ");
		int n=sc.nextInt();
		if(n>0){
			System.out.println("Given number "+n+" is positive number");
		}
		else if(n==0){
			System.out.println("Given number "+n+" is Zero");
		}
		else{
			System.out.println("given number "+n+" is negative number");
		}
	}
}
